{
  "name": "teamup-webapp",
  "homepage": ".",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run lint:fix && npm run test-husky",
    "post-checkout": "npm install",
    "format": "prettier --write .",
    "test-husky": "react-scripts test --watchAll=false --passWithNoTests",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "start-clean": "npm ci && npm run start",
    "build-prod": "npm ci && npm run format && npm run build",
    "serve": "serve build -n",
    "serve-clean": "npm run build-prod && npm run serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "node-sass": "^6.0.1",
    "npx": "^10.2.2",
    "prettier": "^2.3.2"
  },
  "lint-staged": {
    "*": [
      "npm run format"
    ]
  }
}
